{
  "env": {
    "server": {
      "presets": ["react"],
      "plugins": [
        "react-imported-component/babel",
        "babel-plugin-dynamic-import-node",
        "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      },
      "@babel/plugin-proposal-decorators",
      {
        "decorators-legacy": true
      }
    ]
    },
    "client": {
      "presets": ["react"],
      "plugins": [
        "react-imported-component/babel",
        "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      },
      "@babel/plugin-proposal-decorators",
      {
        "decorators-legacy": true
      }
    ]
    }
  }
}
