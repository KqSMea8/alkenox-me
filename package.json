  "scripts": {
    "dev": "rimraf dist .cache/ && yarn generate-imported-component && parcel -p 3000 app/index.html",
    "dev:server": "babel-watch ./server/server.js",
    "build": "rimraf dist .cache && yarn generate-imported-component && yarn build-client && yarn build-server",
    "build-client": "cross-env BABEL_ENV=client parcel build app/index.html -d dist/client --public-url /dist",
    "build-server": "cross-env BABEL_ENV=server parcel build server/index.js -d dist/server --public-url /dist --target=node6",
    "generate-imported-component": "imported-components app app/imported.ts",
    "favicon": "unite-image pngsToIco --sourceFolder=./app --sourceFiles=favicon.png --destFile=./app/favicon.ico",
    "serve": "node dist/server/"
  },
  "proxy": "http://localhost:3001",
  "assetsPath": "./app/api",
