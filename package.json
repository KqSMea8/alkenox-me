{
  "name": "alkenox-me",
  "version": "0.4.0",
  "description": "Example of SSR with React and ParcelJS",
  "main": "alkenox.index.js",
  "keywords": [
    "parcel",
    "react",
    "ssr"
  ],
  "author": "n0-x <Alkenox Media & Entertainment, LLC.>",
  "license": "MIT",
  "repository": "reactivestack/parcel-react-ssr",
  "sitemap": {
    "siteURL": "https://alkenox-me.com/"
  },
  "prerender": {
    "routes": [
      "/",
      "/content-creation",
      "/video-production",
      "/interactive-media",
      "/sound-design",
      "/music-production",
      "/web-design",
      "/motion-graphics",
      "/audio-visual-rx",
      "/about-us",
      "/contact-us"
    ]
  },
  "staticFiles": {
    "staticPath": "./app/static/",
    "watcherGlob": "**"
  },
  "scripts": {
    "dev": "yarn generate-imported-component && parcel app/index.html",
    "build": "rimraf dist && yarn generate-imported-component && yarn build-client && yarn build-server",
    "build-client": "cross-env BABEL_ENV=client parcel build app/index.html -d dist/client --public-url /dist",
    "build-server": "cross-env BABEL_ENV=server parcel build server/index.js -d dist/server --public-url /dist --target=node6",
    "generate-imported-component": "imported-components app app/imported.js",
    "start": "node dist/server"
  },
  "dependencies": {
    "@abide-community/parcel-plugin-clean-out-dir": "^1.0.0",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@mdx-js/parcel-plugin-mdx": "^0.16.8",
    "@mdx-js/tag": "^0.13.1-1",
    "@svgr/cli": "^4.1.0",
    "@svgr/core": "^4.1.0",
    "@svgr/parcel-plugin-svgr": "^4.1.0",
    "apollo-boost": "^0.1.27",
    "apollo-client": "^2.4.12",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.2.1",
    "cheerio": "^1.0.0-rc.2",
    "classnames": "^2.2.6",
    "compression": "^1.7.2",
    "cors": "^2.8.5",
    "create-react-class": "^15.6.3",
    "d3": "^5.7.0",
    "express": "^4.16.2",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.4",
    "handlebars": "^4.0.12",
    "handlebars-to-jsx": "^0.0.7",
    "jest": "^24.1.0",
    "jquery": "^3.3.1",
    "llog": "^0.2.0",
    "lorem-ipsum": "^1.0.6",
    "markdown-to-jsx": "^6.9.1",
    "memoize-one": "5",
    "mobx": "^5.9.0",
    "mobx-react": "^5.4.3",
    "mobx-react-devtools": "^6.0.3",
    "mobx-react-form": "^1.36.1",
    "mobx-react-form-devtools": "^1.10.0",
    "mongodb": "^3.1.13",
    "nodemailer": "^5.1.1",
    "nodemailer-express-handlebars": "^3.0.0",
    "nodemailer-smtp-transport": "^2.7.4",
    "normalizr": "^3.3.0",
    "parcel-plugin-bundle-manifest": "^0.2.0",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "parcel-plugin-sitemap": "^2.0.1",
    "parcel-plugin-static-files-copy": "^2.0.0",
    "passport": "^0.4.0",
    "popper.js": "^1.14.6",
    "prop-types": "^15.6.2",
    "react": "^16.8.1",
    "react-apollo": "^2.4.1",
    "react-dom": "^15.6.1 || 16",
    "react-focus-lock": "^1.17.7",
    "react-form": "^3.5.6",
    "react-helmet": "^5.2.0",
    "react-helmet-async": "^0.2.0",
    "react-image": "^2.0.0",
    "react-imported-component": "^4.0.1",
    "react-modular-audio-player": "^1.1.1",
    "react-player": "^1.9.3",
    "react-portal": "^4.2.0",
    "react-pose": "^4.0.6",
    "react-reveal": "^1.2.2",
    "react-router-dom": "^4.3.1",
    "react-scroll-percentage": "^3.1.0",
    "react-show": "^3.0.4",
    "react-transition-group": "^2.5.3",
    "react-transition-group-v2": "^2.4.0",
    "react-video-renderer": "^2.4.2",
    "reactstrap": "^7.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "cross-env": "^5.1.3",
    "eslint": "^4.19.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "node-sass": "^4.7.2",
    "parcel-bundler": "^1.6.1",
    "parcel-plugin-prerender": "^1.1.0",
    "react-hot-loader": "^4.0.0",
    "rimraf": "^2.6.2"
  }
}
