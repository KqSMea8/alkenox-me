{
  "name": "tools",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "parcel:dev": "rimraf ../dist/dev && parcel -p 3000 ../web/app/index.html -d ../dist/dev",
    "dev:build": "rimraf dist .cache && yarn generate-imported-component && yarn build-client && yarn build-server",
    "build:client": "cross-env BABEL_ENV=client parcel build ../web/app/index.html -d dist/client --public-url /dist",
    "build:server": "cross-env BABEL_ENV=server parcel build ../web/server/index.js -d dist/server --public-url /dist --target=node6",
    "serve": "node ../web/dist/server/",
    "favicon": "unite-image pngsToIco --sourceFolder=./app --sourceFiles=favicon.png --destFile=./app/favicon.ico"
  },
  "proxy": "http://localhost:3001",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/polyfill": "^7.2.5",
    "@svgr/parcel-plugin-svgr": "^4.1.0",
    "@types/parcel-bundler": "^1.10.2",
    "autoprefixer": "^9.5.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-emotion": "^10.0.9",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-mobx-deep-action": "^1.6.1",
    "browserslist": "^4.5.0",
    "cross-env": "^5.2.0",
    "jest": "^24.5.0",
    "jest-emotion": "^10.0.9",
    "jest-haste-map": "^24.5.0",
    "jest-resolve": "^24.5.0",
    "node-sass": "^4.7.2",
    "parcel-bundler": "^1.6.1",
    "parcel-plugin-asset-copier": "^1.0.0",
    "parcel-plugin-bundle-manifest": "^0.2.0",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "parcel-plugin-imagemin": "^2.1.3",
    "parcel-plugin-static-files-copy": "^2.0.0",
    "parcel-plugin-typed-css-modules": "^1.1.0",
    "rimraf": "^2.6.3",
    "sass": "^1.17.3",
    "ts-jest": "^24.0.0",
    "unitejs-image": "^1.0.2"
  },
  "dependencies": {}
}
